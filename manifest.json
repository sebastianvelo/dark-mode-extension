{
    "manifest_version": 2,
    "name": "Dark Mode",
    "description": "Activates dark mode in some websites",
    "version": "1.0",
    "icons": {
        "128": "assets/128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "assets/icon.png"
    },
    "permissions": [
        "activeTab"
    ]
}